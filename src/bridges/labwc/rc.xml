		<keybind bridge="wm.keybindings/move-to-workspace-last" key="W-S-End">
			<action name="SendToDesktop" to="1" wrap="no" />
		</keybind>
		<keybind bridge="wm.keybindings/move-to-workspace-1" key="W-S-Home">
			<action name="SendToDesktop" to="1" wrap="no" />
		</keybind>
		<keybind bridge="wm.keybindings/move-to-workspace-2" key="undefined">
