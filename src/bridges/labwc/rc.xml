		<keybind bridge="wm.keybindings/switch-to-workspace-3" key="undefined">
			<action name="GoToDesktop" to="3" />
		</keybind>
		<keybind bridge="wm.keybindings/switch-to-workspace-4" key="undefined">
			<action name="GoToDesktop" to="4" />
		</keybind>
		<keybind bridge="wm.keybindings/switch-to-workspace-left" key="W-Page_Up">
			<action name="GoToDesktop" to="left" wrap="no" />
		</keybind>
		<keybind bridge="wm.keybindings/switch-to-workspace-right" key="W-Page_Down">
			<action name="GoToDesktop" to="right" wrap="no" />
