		</keybind>
		<keybind bridge="plugins.media-keys/previous" key="undefined">
			<action name="" command="" />
		</keybind>
		<keybind bridge="plugins.media-keys/stop" key="undefined">
			<action name="" command="" />
		</keybind>
		<keybind bridge="plugins.media-keys/volume-down" key="undefined">
			<action name="Execute" command="wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-" />
		</keybind>
		<keybind bridge="plugins.media-keys/volume-mute" key="undefined">
			<action name="Execute" command="wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle" />
		</keybind>
		<keybind bridge="plugins.media-keys/volume-up" key="undefined">
			<action name="Execute" command="wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+" />
		</keybind>
		<keybind bridge="plugins.media-keys/screensaver" key="W-l">
			<action name="Execute" command="dbus-send --type=method_call --dest=org.buddiesofbudgie.BudgieScreenlock /org/buddiesofbudgie/Screenlock org.buddiesofbudgie.BudgieScreenlock.Lock" />
		</keybind>
		<keybind bridge="plugins.media-keys/logout" key="C-A-Delete">
			<action name="Execute" command="budgie-session-quit" />
		</keybind>
		<keybind key="S-W-p">
			<action name="Execute" command="dbus-send --type=method_call --dest=org.buddiesofbudgie.PowerDialog /org/buddiesofbudgie/PowerDialog org.buddiesofbudgie.PowerDialog.Toggle" />
		</keybind>
		<keybind bridge="wm.keybindings/panel-run-dialog" key="A-F2">
			<action name="Execute" command="budgie-run-dialog" />
		</keybind>
		<keybind key="A-Space">
			<action name="ShowMenu" menu="client-menu" />
		</keybind>
		<keybind bridge="wm.keybindings/switch-input-source" key="A-Shift_L">
			<action name="" command="" />
		</keybind>
		<keybind bridge="wm.keybindings/switch-input-source-backward" key="W-S-space">
			<action name="" command="" />
		</keybind>
		<keybind bridge="wm.keybindings/close" key="A-F4">
			<action name="Close" />
		</keybind>
		<keybind bridge="wm.keybindings/minimize" key="W-h">
			<action name="Iconify" />
		</keybind>
		<keybind bridge="wm.keybindings/lower" key="undefined">
			<action name="Lower" />
		</keybind>
		<keybind bridge="wm.keybindings/maximize" key="W-Up">
			<action name="Maximize" direction="both" />
		</keybind>
		<keybind bridge="wm.keybindings/maximize-horizontally" key="undefined">
			<action name="Maximize" direction="right" />
		</keybind>
		<keybind bridge="wm.keybindings/maximize-vertically" key="undefined">
			<action name="Maximize" direction="left" />
		</keybind>
		<keybind bridge="wm.keybindings/begin-move" key="A-F7">
			<action name="" command="" />
		</keybind>
		<keybind bridge="wm.keybindings/raise" key="undefined">
			<action name="Raise" />
		</keybind>
		<keybind bridge="wm.keybindings/raise-or-lower" key="undefined">
			<action name="" command="" />
		</keybind>
		<keybind bridge="wm.keybindings/begin-resize" key="A-F8">
			<action name="Resize" />
		</keybind>
		<keybind bridge="wm.keybindings/unmaximize" key="W-Down">
			<action name="UnMaximize" direction="both" />
		</keybind>
		<keybind bridge="wm.keybindings/toggle-fullscreen" key="undefined">
			<action name="ToggleFullscreen" />
		</keybind>
		<keybind bridge="wm.keybindings/toggle-maximized" key="A-F10">
			<action name="ToggleMaximize" direction="both" />
		</keybind>
		<keybind bridge="wm.keybindings/toggle-on-all-workspaces" key="undefined">
			<action name="" command="" />
		</keybind>
		<keybind bridge="mutter.keybindings/toggle-tiled-left" key="W-Left">
			<action name="ToggleSnapToEdge" direction="left" />
		</keybind>
		<keybind bridge="mutter.keybindings/toggle-tiled-right" key="W-Right">
			<action name="ToggleSnapToEdge" direction="right" />
		</keybind>
		<keybind key="XF86MonBrightnessUp">
			<action name="Execute" command="dbus-send --session --type=method_call --dest=org.gnome.SettingsDaemon.Power /org/gnome/SettingsDaemon/Power org.gnome.SettingsDaemon.Power.Screen.StepUp " />
		</keybind>
		<keybind key="XF86MonBrightnessDown">
			<action name="Execute" command="dbus-send --session --type=method_call --dest=org.gnome.SettingsDaemon.Power /org/gnome/SettingsDaemon/Power org.gnome.SettingsDaemon.Power.Screen.StepDown " />
		</keybind>
		<keybind bridge="custom1" key="C-A-o">
			<action name="Execute" command="mate-sysmonitor" />
		</keybind>
	</keyboard>
	<theme>
		<name>theme_name</name>
		<dropShadows>yes</dropShadows>
		<font place="ActiveWindow" name="font name" weight="Bold" size="11" slant="Normal" />
		<font place="InactiveWindow" name="font name" weight="Bold" size="11" slant="Normal" />
		<titlebar>
			<layout>:iconify,max,close:</layout>
		</titlebar>
	</theme>
	<windowRules>
		<windowRule identifier="Nemo-desktop" title="Desktop" skipTaskbar="yes" skipWindowSwitcher="yes">
			<action name="ToggleAlwaysOnBottom" />
		</windowRule>
		<windowRule identifier="nemo-desktop" title="nemo-desktop" skipTaskbar="yes" skipWindowSwitcher="yes">
			<action name="ToggleAlwaysOnBottom" />
		</windowRule>
		<windowRule identifier="budgie-power-dialog" title="budgie-power-dialog" skipTaskbar="yes" skipWindowSwitcher="yes">
			<action name="AutoPlace" policy="center" />
		</windowRule>
		<windowRule identifier="budgie-daemon" title="budgie-daemon" skipTaskbar="yes" skipWindowSwitcher="yes">
			<action name="AutoPlace" policy="center" />
		</windowRule>
	</windowRules>
	<placement>
		<policy>automatic</policy>
	</placement>
	<focus>
		<followMouse>no</followMouse>
		<raiseOnFocus>no</raiseOnFocus>
	</focus>
	<windowSwitcher allWorkspaces="no" />
	<mouse>
		<doubleClickTime>400</doubleClickTime>
	</mouse>
	<libinput>
		<device category="touchpad">
			<naturalScroll>no</naturalScroll>
			<leftHanded>yes</leftHanded>
			<pointerSpeed>0.0</pointerSpeed>
			<accelProfile>flat</accelProfile>
			<tap>yes</tap>
			<tapButtonMap>default</tapButtonMap>
			<tapAndDrag>yes</tapAndDrag>
			<dragLock>no</dragLock>
			<middleEmulation>no</middleEmulation>
			<disableWhileTyping>yes</disableWhileTyping>
			<clickMethod>clickfinger</clickMethod>
			<sendEventsMode>yes</sendEventsMode>
		</device>
		<device category="non-touch">
			<naturalScroll>no</naturalScroll>
			<leftHanded>no</leftHanded>
			<pointerSpeed>0.051094890510948954</pointerSpeed>
			<accelProfile>adaptive</accelProfile>
			<middleEmulation>no</middleEmulation>
		</device>
	</libinput>
</labwc_config>
